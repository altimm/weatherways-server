{
  "name": "register-api",
  "version": "0.0.1",
  "description": "API for a basic register. RESTful web services using restify written in typescript.",
  "engines": {
    "node": "^12.4.0",
    "npm": "^6.9.0"
  },
  "keywords": [
    "nodejs",
    "restify",
    "sequelize",
    "typescript"
  ],
  "author": "rxp01@uark.edu",
  "license": "MIT",
  "scripts": {
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build-ts && npm run tslint",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --forceExit --coverage --verbose",
    "postinstall": "npm run rebuild"
  },
  "dependencies": {
    "async": "^3.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^7.0.0",
    "errorhandler": "^1.5.1",
    "restify": "^8.3.3",
    "pg": "^7.11.0",
    "sequelize": "^4.44.0",
    "moment": "^2.24.0",
    "express": "^5.0.0-alpha.7",
    "morgan": "^1.9.1",
    "bunyan": "^2.0.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "@types/restify": "^7.2.11",
    "@types/supertest": "^2.0.7",
    "@types/sequelize": "^4.28.3",
    "chai": "^4.2.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
  }
}
